    "@subsquid/evm-typegen": "^3.2.2",
    "@subsquid/graphql-server": "^4.3.0",
    "@subsquid/ss58": "^0.1.4",
    "@subsquid/typeorm-migration": "^1.2.1",
    "@subsquid/typeorm-store": "^1.2.2",
    "@subsquid/evm-processor": "^1.8.1",
    "assert": "^2.0.0",
    "big.js": "^6.2.1",
    "dotenv": "^16.0.3",
    "ethers": "^6.3.0",
    "pg": "^8.8.0",
    "typeorm": "^0.3.17"
